<h1> Edit Sale Order</h1>
<form action="/subscription/save" method="post">
    <input type="hidden" name="subscription_id" value="<%= onerec.subscription_id %>" >
    <table>
        <tr> <td> Customer ID: </td> <td> <input type="int" name="customer_id" required> </td> </tr>
        <tr> <td> Category ID: </td> <td> <input type="int" name="category_id" required> </td> </tr>
        <tr> <td> Subscribe Date: </td> <td> <input type="date" name="subscribedate" required> </td> </tr>
        <tr> <td> Unsubscribe Date: </td> <td> <input type="date" name="unsubscribedate" required> </td> </tr>
        </table>
    <button type="submit">Save</button>
</form>
