<h1> Edit Order Detail</h1>
<form action="/orderdetail/save" method="post">
    <input type="hidden" name="orderdetail_id" value="<%= onerec.orderdetail_id %>" >
    <table>
        <tr> <td> Order ID: </td> <td> <input type="int" name="order_id" required> </td> </tr>
        <tr> <td> Product ID: </td> <td> <input type="int" name="product_id" required> </td> </tr>
        <tr> <td> Sale Price: </td> <td> <input type="number" name="saleprice" required> </td> </tr>
        <tr> <td> Quantity: </td> <td> <input type="int" name="qty" required> </td> </tr>
    </table>
    <button type="submit">Save</button>
</form>
